            <!-- Mobile menu toggle, controls the 'mobileMenuOpen' state. -->
            <button @click="open = true" type="button" class="relative rounded-md text-gray-200 sm:hidden">
                <span class="absolute -inset-0.5"></span>
                <span class="sr-only">Open menu</span>
                <svg class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" aria-hidden="true">
                <path stroke-linecap="round" stroke-linejoin="round" d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5" />
                </svg>
            </button>

            <!-- Logo -->
            <div class="ml-4 flex lg:ml-0">
                <div>
                <span class="sr-only">MDP</span>
                <a href="#home"><img class="h-8 w-auto" src="/imgs/logo.png" alt="mike-d-pascal"></a>
                </div>
            </div>

            <!-- Flyout menus -->
            <div class="hidden sm:ml-8 sm:block sm:self-stretch">
