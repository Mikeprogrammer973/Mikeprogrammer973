
<div x-data=" {open: false} " class="sticky top-0 left-0 z-20">
    <!--
    Mobile menu

    Off-canvas menu for mobile, show/hide based on off-canvas menu state.
    -->
    <div x-show="open" class="relative z-40 block sm:hidden" role="dialog" aria-modal="true">
    <!--
        Off-canvas menu backdrop, show/hide based on off-canvas menu state.

        Entering: "transition-opacity ease-linear duration-300"
        From: "opacity-0"
        To: "opacity-100"
        Leaving: "transition-opacity ease-linear duration-300"
        From: "opacity-100"
        To: "opacity-0"
    -->
    <div class="fixed inset-0 bg-black bg-opacity-25"></div>

    <div class="fixed inset-0 z-40 flex">
        <div @click.outside="open = false" class="relative flex w-full max-w-xs flex-col overflow-y-auto bg-gray-800 pb-12 shadow-xl">
        <div class="flex px-4 pb-2 pt-5">
            <button @click="open = false" type="button" class="relative m-2 inline-flex items-center justify-center rounded-md p-2 text-gray-400">
            <span class="absolute -inset-0.5"></span>
            <span class="sr-only">Close menu</span>
            <svg class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" aria-hidden="true">
                <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12" />
            </svg>
            </button>
        </div>

        <div class="space-y-6 border-t border-gray-700 px-4 py-6">
            <div class="flow-root">
            <button class="m-op-06 cursor-default"><a @click="open = false" class="cursor-pointer" href="dashboard">Dashboard</a></button>
            </div>
            <div class="flow-root">
            <button class="m-op-01 cursor-default"><a @click="open = false" class="cursor-pointer" href="home">Home</a></button>
            </div>
            <div class="flow-root">
            <button class="m-op-02 cursor-default"><a @click="open = false" class="cursor-pointer" href="about">About</a></button>
            </div>
            <div class="flow-root">
            <button class="m-op-03 cursor-default"><a @click="open = false" class="cursor-pointer" href="skills">Skills</a></button>
            </div>
            <div class="flow-root">
            <button class="m-op-04 cursor-default"><a @click="open = false" class="cursor-pointer" href="experiences">Experiences</a></button>
            </div>
        </div>

        </div>
    </div>
    </div>

