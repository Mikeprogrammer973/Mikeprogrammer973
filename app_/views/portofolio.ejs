      @keyframes teste {
        0%, 100% {
          transform: translateY(-2%);
          animation-timing-function: cubic-bezier(0.8,0,1,1);
        }
        50% {
            transform: none;
            animation-timing-function: cubic-bezier(0,0,0.2,1);
        }
      }
      select{
        background-color: inherit;
        padding: 2px;
        border: none;
        outline: double currentColor;
        border-radius: 7px;
      }
      .skl-p-c {
        height: 150px;
        width: 150px;
        position: relative;
        overflow: hidden;
        margin: 20px;
        border-radius: 50%;
        transform: rotate3d(0, 0, 1, 0.001deg); /* clear artifacts*/
      }
      .skl-p-c:before,
      .skl-p-c:after {
        content: " ";
        position: absolute;
        left: 0;
        top: 0;
        width: 150px;
        height: 150px;
        clip: rect(0px, 75px, 150px, 0px);
        background-color: inherit;
        transform: rotate3d(0, 0, 1, 0deg);
        z-index: 2;
      }
      .spin-c-1:before,
      .spin-c-1:after {
        animation: 2s spin-c-1 linear 4s forwards;
      }
      .spin-c-2:before,
      .spin-c-2:after {
        animation: 2s spin-c-2 linear 4s forwards;
      }
      .spin-c-3:before,
      .spin-c-3:after {
        animation: 2s spin-c-3 linear 4s forwards;
      }
      .spin-c-4:before,
      .spin-c-4:after {
        animation: 2s spin-c-4 linear 4s forwards;
      }
      .skl-p-c:after {
        content: " ";
        background: currentColor;
        z-index: 1;
        animation: 2s spin linear 2s forwards;
      }
      
      @keyframes spin {
        from {
          transform: rotate3d(0, 0, 1, -0deg);
        }
        to {
          transform: rotate3d(0, 0, 1, -180deg);
        }
      }
      @keyframes spin-c-1 {
        from {
          background: currentColor;
          transform: rotate3d(0, 0, 1, -180deg);
        }
        to {
          background: currentColor;
          transform: rotate3d(0, 0, 1, -340deg);
        }
      }
      @keyframes spin-c-2 {
        from {
          background: currentColor;
          transform: rotate3d(0, 0, 1, -180deg);
        }
        to {
          background: currentColor;
          transform: rotate3d(0, 0, 1, -329deg);
        }
      }
      @keyframes spin-c-3 {
        from {
          background: currentColor;
          transform: rotate3d(0, 0, 1, -180deg);
        }
        to {
          background: currentColor;
          transform: rotate3d(0, 0, 1, -350deg);
